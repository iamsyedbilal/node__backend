meta {
  name: getAllTours
  type: http
  seq: 1
}

get {
  url: http://localhost:3000/api/v1/tours
  body: none
  auth: inherit
}

params:query {
  ~sort: avgRating,-price
  ~page: 1
  ~limit: 3
  ~avgRating[gte]: 4.5
  ~price[lt]: 500
  ~fields: name,duration,description
}

settings {
  encodeUrl: true
  timeout: 0
}
